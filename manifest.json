
{
  "manifest_version": 2,
  "name": "forceJump",
  "version": "1.0.0",
  "description": "make you free for link-jump",
  "icons":
  {
      "16": "images/24x24.png",
      "48": "images/38x38.png",
      "128": "images/38x38.png"
  },
  "page_action":{
      "default_icon":
      {
          "24": "images/24x24.png",
          "38": "images/38x38.png"
      }
  },
  "author": "Polaris https://www.ttangg.com",
  "background":
  {
      "scripts": [
          "scripts/background.js"
      ]
  },
  "content_scripts": [
      {
        "matches": ["https://*.csdn.net/*", "https://*.juejin.cn/*", "https://*.jianshu.com/*"],
        "js": ["scripts/background.js"]
      }
  ],
  "permissions": []
}
